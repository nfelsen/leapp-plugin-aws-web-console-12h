{
  "name": "aws-web-console-12h",
  "author": "Nathaniel Felsen",
  "version": "0.0.2",
  "description": "Create an AWS web console with a session expiration set to 12H",
  "keywords": [
    "leapp-plugin",
    "AWS"
  ],
  "leappPlugin": {
    "supportedOS": [
      "mac",
      "windows",
      "linux"
    ],
    "supportedSessions": [
      "awsIamRoleFederated",
      "awsIamRoleChained",
      "awsSsoRole",
      "aws"
    ]
  },
  "scripts": {
    "build": "webpack --config webpack.config.js"
  },
  "files": [
    "plugin.js"
  ],
  "license": "MPL",
  "dependencies": {
    "@noovolari/leapp-core": "^0.1.117"
  },
  "devDependencies": {
    "copy-webpack-plugin": "^11.0.0",
    "path-browserify": "^1.0.1",
    "ts-loader": "^9.3.1",
    "typescript": "^4.7.4",
    "webpack": "^5.74.0",
    "webpack-cli": "^4.10.0"
  }
}
